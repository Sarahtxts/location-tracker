{
  "expo": {
    "name": "Location Tracker",
    "slug": "location-tracker-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "locationtracker",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.locationtracker.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs your location to track check-ins and check-outs.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs your location to track check-ins and check-outs even when the app is in the background."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyC1Ec-gl1zZ-bn2qGWY95BtidYkIPhDei8"
      }
    },
    "android": {
      "package": "com.locationtracker.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "INTERNET"
      ],
      "useNextNotificationsApi": true,
      "intentFilters": [],
      "jsEngine": "hermes",
      "softwareKeyboardLayoutMode": "pan",
      "allowHttp": true,
      "usesCleartextTraffic": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC1Ec-gl1zZ-bn2qGWY95BtidYkIPhDei8"
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Location Tracker to use your location for check-ins and check-outs."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0710aeab-8530-4abb-a158-da6f8d54876c"
      }
    }
  }
}